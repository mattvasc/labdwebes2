<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>works</servlet-name>
        <jsp-file>/works.jsp</jsp-file>
    </servlet>
    <servlet>
        <servlet-name>ListaGenero</servlet-name>
        <servlet-class>Controller.ListaGenero</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Ranking</servlet-name>
        <servlet-class>Controller.Ranking</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TestePaginacao</servlet-name>
        <servlet-class>Controller.TestePaginacao</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Avancada</servlet-name>
        <servlet-class>Controller.Avancada</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>works</servlet-name>
        <url-pattern>/works</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>services</servlet-name>
        <jsp-file>/services.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>services</servlet-name>
        <url-pattern>/services</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>about</servlet-name>
        <jsp-file>/about.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>about</servlet-name>
        <url-pattern>/about</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ListaGenero</servlet-name>
        <url-pattern>/ListaGenero</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Ranking</servlet-name>
        <url-pattern>/Ranking</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>pagination</servlet-name>
        <jsp-file>/testePagination.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>pagination</servlet-name>
        <url-pattern>/pagination</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TestePaginacao</servlet-name>
        <url-pattern>/TestePaginacao</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Avancada</servlet-name>
        <url-pattern>/Avancada</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
